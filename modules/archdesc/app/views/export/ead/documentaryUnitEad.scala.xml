@(tree: utils.ead.DocTree)(implicit lang: Lang)

<ead>
    @header(tree)
    <archdesc level="@tree.item.model.primaryDescription.flatMap(_.identity.levelOfDescription).getOrElse("otherlevel")">
        @desc(tree)
        <dsc>
            @for(t <- tree.children) {
                @eadLevel(1, t)
            }
        </dsc>
    </archdesc>
</ead>