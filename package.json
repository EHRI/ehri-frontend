  "jest": {
    "globals": {
      "CONFIG": {},
      "SERVICE": {},
      "__languageData": {},
      "LocaleData": {}
    },
    "testEnvironment": "jsdom",
    "collectCoverage": false,
    "moduleFileExtensions": [
      "js",
      "json",
      "ts",
      "node",
      "vue"
    ],
    "moduleNameMapper": {
      "\\.(css|less)$": "<rootDir>/modules/admin/app/assets/js/__mocks__/styleMock.js"
    },
    "testMatch": [
      "**/__tests__/**/*.[jt]s?(x)",
      "**/?(*.)+(spec|test).[tj]s?(x)"
    ],
    "modulePathIgnorePatterns": [
      "/target/",
      "/.+/target/"
    ],
    "testPathIgnorePatterns": [
      "/node_modules/",
      "/target/",
      "/.+/target/"
    ],
    "transform": {
      "^.+\\.(ts|tsx)$": "ts-jest",
      ".*\\.(vue)$": "vue-jest"
    }
  }
