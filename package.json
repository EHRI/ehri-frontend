    "corejs-typeahead": "^1.3.1",
    "handlebars": "^4.4.3",
    "jquery": "^3.4.1",
    "jquery-flot": "^0.8.3",
    "jquery-hoverintent": "^1.10.1",
    "jquery-placeholder": "^2.3.1",
    "jquery-validation": "^1.19.5",
    "js-cookie": "^2.2.1",
    "leaflet": "^1.7.1",
    "leaflet-curve": "^1.0.0",
    "lodash": "^4.17.21",
    "luxon": "^1.25.0",
    "markdown-it": "^14.1.0",
    "material-design-icons": "^3.0.1",
    "papaparse": "^5.3.2",
    "select2": "^4.0.13",
    "sortablejs": "^1.13.0",
    "tiny-emitter": "^2.1.0",
